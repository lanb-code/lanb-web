<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.colodoo.manager.test.service.TestRelationMapper">

	<select id="getList" resultType="com.colodoo.manager.test.model.TestVO">
		SELECT 
		a.TEST_ID AS testId,
		a.TEST_NAME AS testName,
		a.CREATE_TIME AS createTime
		
		FROM test a
	</select>

</mapper>